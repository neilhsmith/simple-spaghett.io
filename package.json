{
  "name": "simple-spaghett.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:all": "npm run lint && npm run type-check && npm run test"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": false
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-results": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-jest": "^27.5.1",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "typescript": "4.6.2"
  }
}
